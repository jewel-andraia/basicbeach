{
    "origin": "#[be:#be#][space:#space#][period:#period#]Be you.capitalizeAll#",
    "Be you": [
        "#be.capitalize##space##adjective##period##be.capitalize##space##[space]you#",
        "#be.capitalize##space##adjective##period##be.capitalize##space##adjective##period##be.capitalize##space##you#"
    ],
    "space": [""," ", "*", "-", "·"],
    "period": [",",".","·","-"],
    "be": [
        "Be",
        "Act",
        "believe"
    ],
    "adjective": [
        "good",
        "happy",
        "positive",
        "dour",
        "sour"
    ],
    "you": [
        "you",
        "yourself",
        "#creature.a.replace(' ', space)#",
        "#proper noun#"
    ],
    "proper noun": [
        "Batman"
    ],
    "creature": [
        "#corpora-animals-common#"
    ],
    "corpora-animals-common": {
        "!::": "corpora",
        "path": ["animals", "common"]
    }
}
