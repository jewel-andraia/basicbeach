    },
    "HH": {
        "!::": "date",
        "method": "HH"
    },
    "MM": {
        "!::": "date",
        "method": "mm"
    },
    "mm": {
        "!::": "date",
        "method": "mm"
    },
    "hour": {
        "!::": "date",
        "method": "hour"
    },
    "minute": {
        "!::": "date",
        "method": "minute"
    },
    "AM": {
        "!::": "date",
        "method": "AM"
    },
    "AMPM": {
        "!::": "date",
        "method": "AMPM"
