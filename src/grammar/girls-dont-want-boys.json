{
	"_attribution": "https://twitter.com/Kendragarden/status/1370432639287496704",
		"origin": ["#human groups.capitalize# don't want #nouns#; they want #foo# and #bar#."],
		"human groups": ["girls", "boys", "women", "men", "babies", "#jobs.s#"],
		"jobs": {
			"!::": "corpora",
			"path": ["humans", "occupations"]
		},
		"nouns": ["#human groups#"],
		"foo": ["#fields of study# #studies#", "#isms of art# #studies#"],
		"bar": ["#food#", "a #place# within #modality# distance of their #place#"],
		"modality": ["walking", "driving", "crawling", "stumbling", "biking"],
		"place": ["hairdresser", "apartment"],
		"studies": ["textbooks", "courses"],
		"fields of study": {
			"!::": "corpora",
			"path": ["books", "academic_subjects", "subjects"]
		},
		"isms of art": {
			"!::": "corpora",
			"path": ["art", "isms"]
		},
		"food": ["#apples# apples", "#beer.s#", "#cocktails.s#"],
		"apples": {
			"!::": "corpora",
			"path": ["foods", "apple_cultivars", "cultivars"]
		},
		"beer": {
			"!::": "corpora",
			"path": ["foods", "beer_styles"]
		},
		"cocktails": {
			"!::": "corpora",
			"path": ["foods", "iba_cocktails", "cocktails"]
		}
}
